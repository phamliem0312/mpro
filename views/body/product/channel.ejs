  <div id="createModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
  
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Create Channel</h4>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Mã dịch vụ</label></div>
              <input class="form-control" type="text" name="Mã dịch vụ" id="service_code">
            </div>
            <div class="col-md-6">
              <div><label for="">Mã đối tác</label></div>
              <input class="form-control" type="text" name="Mã đối tác" disable id="customer_code">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Tên dịch vụ</label></div>
              <input class="form-control" type="text" name="Tên dịch vụ" id="service_name">
            </div>
            <div class="col-md-6">
              <div><label for="">Kênh</label></div>
              <input class="form-control" type="text" name="Kênh" id="channel">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Mô tả dịch vụ</label></div>
              <input class="form-control" type="text" name="Mô tả dịch vụ" id="description">
            </div>
            <div class="col-md-6">
              <div><label for="">Đường dẫn dịch vụ</label></div>
              <input class="form-control" type="text" name="Đường dẫn dịch vụ" id="service_url">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Trạng thái</label></div>
              <select class="form-control" name="Trạng thái" id="status">
                <option value="1">Hiệu lực</option>
                <option value="0">Hết hiệu lực</option>
              </select>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" onclick="save_channel()">Save</button>
        </div>
      </div>
  
    </div>
  </div>

  <div id="updateModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
  
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Chỉnh sửa dịch vụ</h4>
        </div>
        <div class="modal-body">
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Mã dịch vụ</label></div>
              <input class="form-control" disabled type="text" name="Mã dịch vụ" id="service_code_update">
            </div>
            <div class="col-md-6">
              <div><label for="">Mã đối tác</label></div>
              <input class="form-control" type="text" name="Mã đối tác" id="customer_code_update">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Tên dịch vụ</label></div>
              <input class="form-control" type="text" name="Tên dịch vụ" id="service_name_update">
            </div>
            <div class="col-md-6">
              <div><label for="">Kênh</label></div>
              <input class="form-control" disabled type="text" name="Kênh" id="channel_update">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Mô tả dịch vụ</label></div>
              <input class="form-control" type="text" name="Mô tả dịch vụ" id="description_update">
            </div>
            <div class="col-md-6">
              <div><label for="">Đường dẫn dịch vụ</label></div>
              <input class="form-control" type="text" name="Đường dẫn dịch vụ" id="service_url_update">
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div><label for="">Trạng thái</label></div>
              <select class="form-control" name="Trạng thái" id="status_update">
                <option value="1">Hiệu lực</option>
                <option value="0">Hết hiệu lực</option>
              </select>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn fis" data-dismiss="modal" onclick="update_channel()">Lưu</button>
        </div>
      </div>
  
    </div>
